{
    "name": "teals-labs",
    "displayName": "TEALS Labs",
    "description": "Extension to manage download of lab files and running tests.",
    "version": "0.0.1",
    "publisher": "teals",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.getLab",
        "onCommand:extension.testMyCode"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.getLab",
                "title": "TEALS: Download a lab file"
            },
            {
                "command": "extension.testMyCode",
                "title": "TEALS: Run tests on my code"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "request": "^2.87.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
